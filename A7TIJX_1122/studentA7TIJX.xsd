<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:element name="keresztnev"></xs:element>
    <xs:element name="vezetkneve"></xs:element>
    <xs:element name="becenev"></xs:element>
    <xs:element name="kor"></xs:element>
    <xs:attribute name="studentid"></xs:attribute>

    <xs:element name="class">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="student">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="keresztnev"/>
                            <xs:element ref="vezetkneve"/>
                            <xs:element ref="becenev"/>
                            <xs:element ref="kor"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute ref="studentid"/>
        </xs:complexType>
    </xs:element>
    
</xs:schema>